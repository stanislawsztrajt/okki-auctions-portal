{
  "kind": "collectionType",
  "collectionName": "comment_reports",
  "info": {
    "name": "commentReport"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user_id": {
      "type": "string",
      "minLength": 20,
      "maxLength": 30
    },
    "user_ip": {
      "type": "string",
      "minLength": 10,
      "maxLength": 30
    },
    "message": {
      "type": "text",
      "minLength": 20,
      "maxLength": 500
    },
    "comment_id": {
      "type": "string",
      "minLength": 20,
      "maxLength": 30
    }
  }
}
